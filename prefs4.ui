<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkAdjustment" id="extension_index">
    <property name="upper">100</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkNotebook" id="main_prefs">
    <property name="height-request">600</property>
    <property name="focusable">1</property>
    <child>
      <object class="GtkNotebookPage">
        <property name="child">
          <object class="GtkGrid">
            <property name="margin-start">15</property>
            <property name="margin-end">15</property>
            <property name="margin-start">15</property>
            <property name="margin-end">15</property>
            <property name="margin-top">15</property>
            <property name="margin-bottom">15</property>
            <property name="row-spacing">10</property>
            <property name="column-homogeneous">1</property>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Max text length (0 to disable)</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSpinButton">
                <property name="focusable">1</property>
                <property name="halign">end</property>
                <property name="text" translatable="yes">0</property>
                <property name="adjustment">max_text_length</property>
                <signal name="value-changed" handler="on_max_text_length_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Update delay (milliseconds)</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSpinButton">
                <property name="focusable">1</property>
                <property name="halign">end</property>
                <property name="text" translatable="yes">50</property>
                <property name="adjustment">update_delay</property>
                <property name="value">50</property>
                <signal name="value-changed" handler="on_update_delay_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
          </object>
        </property>
        <property name="tab">
          <object class="GtkLabel">
            <property name="label" translatable="yes">General</property>
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkNotebookPage">
        <property name="position">1</property>
        <property name="child">
          <object class="GtkGrid">
            <property name="margin-start">15</property>
            <property name="margin-end">15</property>
            <property name="margin-start">15</property>
            <property name="margin-end">15</property>
            <property name="margin-top">15</property>
            <property name="margin-bottom">15</property>
            <property name="row-spacing">10</property>
            <property name="column-homogeneous">1</property>
            <child>
              <object class="GtkSwitch">
                <property name="focusable">1</property>
                <property name="halign">end</property>
                <signal name="state-set" handler="on_hide_text_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Hide player icon</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSwitch">
                <property name="focusable">1</property>
                <property name="halign">end</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Hide controls</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSwitch">
                <property name="focusable">1</property>
                <property name="halign">end</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Hide seperators</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSwitch">
                <property name="focusable">1</property>
                <property name="halign">end</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Hide text</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Colored player icon</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">4</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSwitch">
                <property name="focusable">1</property>
                <property name="halign">end</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">4</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Show hidden content on hover</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">5</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSwitch">
                <property name="focusable">1</property>
                <property name="halign">end</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">5</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="preset_radio_btn">
                <property name="label" translatable="yes">Preset</property>
                <property name="halign">start</property>
                <property name="active">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">7</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="custom_radio_btn">
                <property name="label" translatable="yes">Custom</property>
                <property name="halign">start</property>
                <property name="group">preset_radio_btn</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">8</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkComboBoxText">
                <property name="halign">end</property>
                <signal name="changed" handler="on_seperator_preset_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">7</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="margin-top">8</property>
                <property name="margin-bottom">8</property>
                <property name="label" translatable="yes">&lt;b&gt;Seperator characters&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">6</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkEntry">
                <property name="focusable">1</property>
                <property name="halign">end</property>
                <property name="placeholder-text" translatable="yes">&lt;...&gt;</property>
                <signal name="changed" handler="on_seperator_custom_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">8</property>
                </layout>
              </object>
            </child>
          </object>
        </property>
        <property name="tab">
          <object class="GtkLabel">
            <property name="label" translatable="yes">Visibility</property>
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkNotebookPage">
        <property name="position">2</property>
        <property name="child">
          <object class="GtkGrid">
            <property name="margin-start">15</property>
            <property name="margin-end">15</property>
            <property name="margin-start">15</property>
            <property name="margin-end">15</property>
            <property name="margin-top">15</property>
            <property name="margin-bottom">15</property>
            <property name="row-spacing">10</property>
            <property name="column-homogeneous">1</property>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Extension position</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkComboBoxText">
                <property name="halign">end</property>
                <items>
                  <item translatable="yes">left</item>
                </items>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Extension index</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSpinButton">
                <property name="focusable">1</property>
                <property name="halign">end</property>
                <property name="text" translatable="yes">0</property>
                <property name="adjustment">extension_index</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">&lt;b&gt;Element order&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkComboBoxText">
                <property name="halign">start</property>
                <signal name="changed" handler="on_element_order_first_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkComboBoxText">
                <property name="halign">center</property>
                <signal name="changed" handler="on_element_order_second_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkComboBoxText">
                <property name="halign">end</property>
                <signal name="changed" handler="on_element_order_third_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
          </object>
        </property>
        <property name="tab">
          <object class="GtkLabel">
            <property name="label" translatable="yes">Position</property>
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkNotebookPage">
        <property name="position">3</property>
        <property name="child">
          <object class="GtkGrid">
            <property name="margin-start">15</property>
            <property name="margin-end">15</property>
            <property name="margin-start">15</property>
            <property name="margin-end">15</property>
            <property name="margin-top">15</property>
            <property name="margin-bottom">15</property>
            <property name="row-spacing">10</property>
            <property name="column-homogeneous">1</property>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">&lt;b&gt;Mouse actions&lt;/b&gt;</property>
                <property name="use-markup">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Left click</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkComboBoxText">
                <property name="halign">end</property>
                <signal name="changed" handler="on_mouse_actions_left_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="halign">start</property>
                <property name="label" translatable="yes">Right click</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkComboBoxText">
                <property name="halign">end</property>
                <signal name="changed" handler="on_mouse_actions_right_changed" swapped="no"/>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
          </object>
        </property>
        <property name="tab">
          <object class="GtkLabel">
            <property name="label" translatable="yes">Other</property>
          </object>
        </property>
      </object>
    </child>
  </object>
  <object class="GtkAdjustment" id="max_text_length">
    <property name="upper">500</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="update_delay">
    <property name="lower">50</property>
    <property name="upper">1000</property>
    <property name="step-increment">50</property>
    <property name="page-increment">10</property>
  </object>
</interface>
